<GameOfLife>
    <Dimension>2</Dimension>
    <GridSize>
        <Size>11</Size>
        <Size>11</Size>
    </GridSize>
    <Cut>axis(x y z) value</Cut>
    <CustomNeighborhoods>
        <Neighborhood>GXX = ((a, b, ...), (c, d, ...), ...)</Neighborhood>
    </CustomNeighborhoods>
    <EvolutionRule>SI(EQ(COMPTER(G0),1), SI(OU(EQ(COMPTER(G8*),2),EQ(COMPTER(G8*),3)),1,0) , SI(EQ(COMPTER(G8*),3),1,0))</EvolutionRule>
    <InitialCells>
        <Cell>x , y , z ...</Cell>
        OR
        <Random>k</Random>
    </InitialCells>
</GameOfLife>